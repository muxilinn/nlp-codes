   "source": [
    "# 输出Python关键字的数量和列表\n",
    "import keyword\n",
    "\n",
    "len_kw = len(keyword.kwlist)\n",
    "print(len_kw)  # 输出关键字数量    \n",
    "\n",
    "print('Python keywords')  # 输出关键字列表\n",
    "i = 0 \n",
    "for k in keyword.kwlist:\n",
    "    if i % 9 == 0 and i != 0:\n",
    "        print()  # 每6个关键字换行\n",
    "    i += 1\n",
    "    # 其他关键词连续输出\n",
    "    print(k, end=' ')"
   ]
  }
